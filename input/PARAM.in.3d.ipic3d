#OUTERBOUNDARY
periodic		xMinBc
periodic		xMaxBc
float			yMinBc
float			yMaxBc
float			zMinBc
float			zMaxBc

#SCHEME
2			nOrder
Rusanov			TypeFlux
minmod			TypeLimiter
1.5			LimiterBeta

#HYPERBOLICDIVB
T			UseHyperbolicDivb
0.02			SpeedHypDim
0.1			HypDecay

#TIMESTEPPING
2                       nStage
0.1                     CflExpl

HALLRESISTIVITY
