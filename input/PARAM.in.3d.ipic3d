0.282095		HallFactor  (m/e = 1/sqrt(4*pi))
1.0			HallCmaxFactor

RESISTIVITY
T			UseResistivity
constant		TypeResistivity
0.01			Eta0Si

#SAVELOGFILE
T			DoSaveLogfile
var			StringLogfile
1			DnSaveLogfile
-1.0			DtSaveLogfile
dt rho mx my mz bx by bz e Pmin Pmax byflux
g

#SAVEINITIAL
T		        DoSaveInitial

#SAVEPLOT
3			nPlotfile
x=0 VAR idl_ascii       PlotString
4           	        DnSavePlot
-20.0             	DtSavePlot
0.0	        	DxSavePlot
rho ux uy uz bx by bz p jx jy jz absdivb
g
y=0 VAR idl_ascii        PlotString
4           	        DnSavePlot
-20.0             	DtSavePlot
0.0	        	DxSavePlot
rho ux uy uz bx by bz p jx jy jz absdivb
g
z=0 VAR idl_ascii        PlotString
4           	        DnSavePlot
-20.0             	DtSavePlot
0.0	        	DxSavePlot
rho ux uy uz bx by bz p jx jy jz absdivb
g

