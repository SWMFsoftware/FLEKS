4			nJ
4			nK
100			MinBlockALL

#GRID
16			nRootBlockX
16			nRootBlockY
8			nRootBlockZ
-12.8			xMin
 12.8			xMax
-6.4			yMin
 6.4			yMax
-1.6			zMin
 1.6			zMax

#UNIFORMSTATE
1.0			rho
0.0			Ux
0.0			Uy
0.0			Uz	 
0.0			Bx
0.0			By
0.015			Bz
0.0			Hyp
0.000157080		p

#PICUNIT
 1.0                    xUnitPicSi
 1.0                    uUnitPicSi

#PICREGION
1                       nPicRegion
-8.6  	                xMinCut
+8.6                    xMaxCut
-2.6                    yMinCut
+2.6                    yMaxCut
-1.0                    zMinCut
+1.0                    zMaxCut
0.2                     DxPic
0.1                     DyPic
0.1                     DyPic

